{
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.rootPatterns": [ ".root", ".vim", ".git", ".hg", ".projections.json" ],
  "diagnostic.errorSign": "✘",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "∆",
  "eslint.enable": true,
  "eslint.run": "onSave",
  "eslint.autoFix": true,
  "eslint.trace.server": "off",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "javascript.suggest.enabled": true,
  "javascript.suggest.paths": true,
  "javascript.suggest.autoImports": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.updateImportsOnFileMove.enable": true,
  "prettier.eslintIntegration": true,
  "prettier.onlyUseLocalVersion": true,
  "prettier.requireConfig": true,
  "snippets.extends": { "javascript.jsx": ["javascript"], "javascriptreact": ["javascript"] },
  "snippets.ultisnips.directories": [ "UltiSnips", "~/comfy_guration/dotfiles/custom_snips" ],
  "suggest.triggerAfterInsertEnter": true,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": false,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.log": "verbose",
  "tsserver.trace.server":  "verbose",
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.validate.enable": true
}
